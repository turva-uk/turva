{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "test": "vitest",
    "test:ci": "vitest --run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "prettier": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "stylelint": "stylelint \"src/**/*.css\"",
    "stylelint:fix": "stylelint \"src/**/*.css\" --fix",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@mantine/core": "^8.2.2",
    "@mantine/form": "^8.2.2",
    "@mantine/hooks": "^8.2.2",
    "@mantine/notifications": "^8.2.2",
    "@tabler/icons-react": "^3.31.0",
    "@types/node": "^24.2.0",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.2.0-next.1",
    "@eslint/js": "^9.30.1",
    "@storybook/addon-a11y": "^10.0.0-beta.10",
    "@storybook/addon-docs": "^10.0.0-beta.10",
    "@storybook/addon-queryparams": "^9.0.1",
    "@storybook/addon-vitest": "^10.0.0-beta.10",
    "@storybook/react-vite": "^10.0.0-beta.10",
    "@testing-library/react": "^16.3.0",
    "@types/lodash": "^4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^10.0.0-beta.10",
    "fetch-mock": "^12.5.4",
    "globals": "^16.3.0",
    "playwright": "^1.54.2",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "storybook": "^10.0.0-beta.10",
    "storybook-addon-remix-react-router": "^5.0.0",
    "stylelint": "^17.0.0",
    "stylelint-config-standard": "^40.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  },
  "imports": {
    "#*": [
      "./*",
      "./*.ts",
      "./*.tsx",
      "./*.js",
      "./*.jsx"
    ]
  }
}
